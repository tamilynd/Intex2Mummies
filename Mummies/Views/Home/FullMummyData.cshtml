                                                <tr colspan="3">
                                                    <td class="style9" colspan="3"><strong>Burial West To Head:</strong></td>
                                                    <td colspan="3">@Model.BurialWestToHead</td>
                                                </tr>
                                                <tr colspan="3">
                                                    <td class="style9" colspan="3"><strong>Burial West To Feet:</strong></td>
                                                    <td colspan="3">@Model.BurialWestToFeet</td>
                                                </tr>
                                                <tr colspan="3">
                                                    <td class="style9" colspan="3"><strong>Burial South To Head:</strong></td>
                                                    <td colspan="3">@Model.BurialSouthToHead</td>
                                                </tr>
                                                <tr colspan="3">
                                                    <td class="style9" colspan="3"><strong>Burial South To Feet:</strong></td>
                                                    <td colspan="3">@Model.BurialSouthToFeet</td>
                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><br /></td>
                                    </tr>
                                    <tr>
                                        <th colspan="3">
                                            <h5>Physical Attributes:</h5>
                                        </th>
                                    </tr>

                                    <tr>
                                        <td class="style8">
                                            <b>Body Analysis:</b>
                                        </td>
                                        <td class="style7">
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_PhotographedLabel">@Model.BodyAnalysis</span>
                                        </td>
                                        <td>
                                            <b>Skull Date:</b>
                                        </td>
                                        <td>
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_Scientific_TestingLabel">@Model.YearOnSkull + @Model.MonthOnSkull + @Model.DateOnSkull</span>
                                        </td>
                                        <td class="style9">
                                            <b>Skull at Magazine:</b>
                                        </td>
                                        <td>
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_SampledLabel">@Model.SkullAtMagazine</span>
                                        </td>
                                        <td>
                                            <b>Postcrania At Magazine:</b>
                                        </td>
                                        <td>
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_Scientific_TestingLabel">@Model.PostcraniaAtMagazine</span>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="style8">
                                            <b>Cribria Orbitala:</b>
                                        </td>
                                        <td class="style7">
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_PhotographedLabel">@Model.CribraOrbitala</span>
                                        </td>
                                        <td class="style9">
                                            <b>Linear Hypoplasia Enamel:</b>
                                        </td>
                                        <td>
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_SampledLabel">@Model.LinearHypoplasiaEnamel</span>
                                        </td>
                                        <td>
                                            <b>Temporal Mandibular Joine Osteoarthritis (TMJOA):</b>
                                        </td>
                                        <td>
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_Scientific_TestingLabel">@Model.TemporalMandibularJointOsteoarthritisTmjOa</span>
                                        </td>

                                        <td>
                                            <b>Postcrania Trauma:</b>
                                        </td>
                                        <td>
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_Scientific_TestingLabel">@Model.PostcraniaTrauma</span>
                                        </td>

                                    </tr>
                                    <tr>
                                        <td><br /></td>
                                    </tr>
                                    <tr>
                                        <th colspan="3">
                                            <h5>Osteology/Archeological Notes:</h5>
                                        </th>
                                    </tr>

                                <td><br /></td>
                                <tr>
                                    <td class="style8" valign="top">
                                        <b>Notes 1</b>:
                                    </td>
                                    <td class="style7" valign="top">
                                        <textarea name="ctl00$ContentPlaceHolder1$FormView1$DescriptionLabel0" rows="2" cols="20" readonly="readonly" id="ctl00_ContentPlaceHolder1_FormView1_DescriptionLabel0" style="height:72px;">@Model.Column73</textarea>
