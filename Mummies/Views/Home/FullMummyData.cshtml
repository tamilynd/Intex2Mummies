@{
}
@model Mummies.Models.MummyDb.FagElGamousDatabaseByLocation
<form asp-action="MummyUpdate" method="Post">
    <div class="content-holder-main"> 

        <table cellspacing="0" border="0" id="ctl00_ContentPlaceHolder1_FormView1" style="border-collapse:collapse;">
            <tbody>
                <tr>
                    <td colspan="2">
                        <div class="content-holder-main">
                            <table class="style1">
                                <tbody>
                                    <tr>
                                        <th colspan="3">
                                            <h5>Basic Mummy Info:</h5>
                                        </th>
                                    </tr>
                                    <tr>

                                        <td class="style8">
                                            <strong>Burial ID: </strong>
                                        </td>
                                        <td class="style7">
                                            <label asp-for="BurialId"> @Model.BurialId </label>
                                        </td>
                                        <td class="style9">
                                            <b>Orientation:</b>
                                        </td>
                                        <td>
                                            <label asp-for="BurialDirection">
                                                @if (Model.BurialDirection == "W")
                                                {
                                                    <a>
                                                        West
                                                    </a>
                                                }
                                                else if (Model.BurialDirection == "E")
                                                {
                                                    <a>
                                                        East
                                                    </a>
                                                }
                                                else
                                                {
                                                    <a>
                                                        -
                                                    </a>
                                                }
                                            </label>
                                        </td>
                                        <td>
                                            <b>Depth:</b>
                                        </td>
                                        <td>
                                            <label asp-for="BurialDepth">
                                                @if (Model.BurialDepth == 0 || Model.BurialDepth is null)
                                                {
                                                    <a>
                                                        -
                                                    </a>
                                                }
                                                else
                                                {
                                                    <a>
                                                        @Model.BurialDepth
                                                    </a>
                                                }
                                            </label>
                                        </td>
                                        <td class="style9">
                                            <b>Wrapping:</b>
                                        </td>
                                        <td>
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_SampledLabel">
                                                @if (Model.BurialWrappingCode is null || Model.BurialWrappingCode == "U")
                                                {
                                                    <a>-</a>
                                                }
                                                else if (Model.BurialWrappingCode == "W")
                                                {
                                                    <a>Full Wrapping</a>
                                                }
                                                else if (Model.BurialWrappingCode == "H")
                                                {
                                                    <a>Partial Wrapping</a>
                                                }
                                                else if (Model.BurialWrappingCode == "B")
                                                {
                                                    <a>Very Little/No Wrapping</a>
                                                }
                                                                                    </span>
                                        </td>

                                    </tr>
                                    <tr>
                                        <td class="style9">
                                            <b>Items:</b>
                                        </td>
                                        <td>
                                            @if (Model.Goods != null)
                                            {
                                                <a>@Model.Goods</a>
                                            }
                                            else
                                            {
                                                <a>No Artifacts</a>
                                            }
                                        </td>
                                        
                                        <td>
                                            <b>Hair Color:</b>
                                        </td>
                                        <td>
                                            @if (Model.HairColorCode is null || Model.HairColorCode == "U")
                                            {
                                                <a>-</a>
                                            }
                                            else
                                            {
                                                <a>@Model.HairColor</a>
                                            }
                                        </td>

                                        <td class="style9">
                                            <b>Age:</b>
                                        </td>
                                        <td>
                                            @if (Model.AgeCodeSingle is null || Model.AgeCodeSingle == "U")
                                            {
                                                <a>-</a>
                                            }
                                            else
                                            {
                                                <a>@Model.Age</a>
                                            }
                                        </td>
                                        <td class="style8">
                                            <b>Bone:</b>
                                        </td>
                                        <td class="style7">
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_PhotographedLabel">Bone Stuff</span>
                                        </td>
                                    </tr>

                                    <tr>
                                        <td><br /></td>
                                    </tr>
                                    <tr>
                                        <th colspan="3">
                                            <h5>Field Book Information:</h5>
                                        </th>
                                    </tr>
                                    <tr>
                                        <td class="style8">
                                            <b>Field Book:</b>
                                        </td>
                                        <td class="style7">
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_PhotographedLabel">@Model.FieldBook</span>
                                        </td>
                                        <td class="style9">
                                            <b>Field Book Page #:</b>
                                        </td>
                                        <td>
                                            <span id="ctl00_ContentPlaceHolder1_FormView1_SampledLabel">@Model.FieldBookPageNumber</span>
                                        </td>
